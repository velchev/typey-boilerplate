{
  "name": "typey-boilerplate",
  "version": "0.0.1",
  "description": "a better typescript project",
  "main": "src/index.tsx",
  "scripts": {
    "start": "cross-env NODE_ENV=development DEBUG=true webpack serve --mode development",
    "build:clean": "rm -rf dist && mkdir dist",
    "build:js": "cross-env NODE_ENV=production DEBUG=false webpack --mode production --progress",
    "build:all": "npm run build:clean && npm run build:js",
    "build": "npm run build:all",
    "prettier": "prettier --write '*/*.{ts,tsx,js,jsx,css,less,json}'",
    "typecheck": "tsc --noEmit -p tsconfig.json",
    "lint": "tsc --noEmit && eslint 'src/*.{ts,tsx,js,jsx,css,less,json}' --quiet --fix",
    "test": "jest"
  },
  "dependencies": {
    "@babel/core": "^7.13.15",
    "@babel/plugin-proposal-class-properties": "^7.13.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.13.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-react": "^7.13.13",
    "@babel/preset-typescript": "^7.13.0",
    "@types/history": "^4.7.8",
    "@types/react": "^17.0.41",
    "@types/react-dom": "^17.0.14",
    "@types/react-redux": "^7.1.16",
    "@types/react-router": "^5.1.13",
    "babel-loader": "^8.2.2",
    "babel-plugin-const-enum": "^1.0.1",
    "connected-react-router": "^6.9.1",
    "core-js": "^3.10.1",
    "history": "^5.0.0",
    "immutable": "^4.0.0-rc.12",
    "normalize-scss": "^7.0.1",
    "raf": "^3.4.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.3",
    "react-router": "^5.2.0",
    "redux": "^4.0.5",
    "seamless-immutable": "^7.1.4"
  },
  "devDependencies": {
    "@redux-devtools/extension": "^3.2.2",
    "@types/jest": "^27.4.1",
    "@types/react-test-renderer": "^17.0.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "autoprefixer": "^10.2.5",
    "babel-jest": "^27.5.1",
    "circular-dependency-plugin": "^5.2.2",
    "compression-webpack-plugin": "^7.1.2",
    "cross-env": "^7.0.3",
    "css-loader": "^5.2.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "cssnano": "^5.0.1",
    "eslint": "^7.24.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "file-loader": "^6.2.0",
    "fork-ts-checker-webpack-plugin": "^6.2.1",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^27.5.1",
    "mini-css-extract-plugin": "^2.3.0",
    "node-sass": "^5.0.0",
    "postcss-loader": "^5.2.0",
    "prettier": "^2.2.1",
    "react-test-renderer": "^17.0.2",
    "sass-loader": "^11.0.1",
    "source-map-loader": "^2.0.1",
    "style-loader": "^2.0.0",
